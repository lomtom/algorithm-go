{
  "title": "基于Go的常用算法",
  "description": "基于Go的常用算法总结",
  "author": "lomtom",
  "plugins": [
    "back-to-top-button",
    "chapter-fold",
    "code",
    "splitter",
    "-lunr",
    "-search",
    "search-pro",
    "custom-favicon",
    "-tbfed-pagefooter",
    "-sharing",
    "sharing-plus",
    "hide-element",
    "github",
    "theme-fexa",
    "copyright"
  ],
  "variables": {
    "themeFexa": {
      "nav": [
        {
          "url": "/",
          "target": "_blank",
          "name": "首页"
        },
        {
          "url": "https://lomtom.cn",
          "target": "_blank",
          "name": "博客"
        }
      ]
    }
  },
  "pluginsConfig": {
    "theme-fexa": {
      "search-placeholder": "输入关键字搜索",
      "logo": "./img/logoko.png",
      "favicon": "./img/logo.ico"
    },
    "github": {
      "url": "https://github.com/lomtom/algorithm-go"
    },
    "hide-element": {
      "elements": [
        ".gitbook-link",
        ".copyright",
        ".book-summary-title"
      ]
    },
    "favicon": "https://lomtom.cn/img/logo.ico",
    "tbfed-pagefooter": {
      "modify_label": "文章修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "copyright": {
      "site": "https://algorithm.lomtom.cn",
      "author": "lomtom",
      "website": "algorithm-go",
      "copyProtect": false
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "twitter": false,
      "weibo": false,
      "all": [
      ]
    }
  }
}